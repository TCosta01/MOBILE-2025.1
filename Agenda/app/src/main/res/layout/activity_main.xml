<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:text="Agenda"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonJaneiro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="50dp"
        android:text="JANEIRO"
        app:layout_constraintBottom_toTopOf="@+id/buttonMarco"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/buttonFevereiro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="70dp"
        android:text="FEVEREIRO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/buttonJaneiro" />

    <Button
        android:id="@+id/buttonMarco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="MARÃ‡O"
        app:layout_constraintStart_toStartOf="@+id/buttonJaneiro"
        app:layout_constraintTop_toBottomOf="@+id/buttonJaneiro" />

    <Button
        android:id="@+id/buttonAbril"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ABRIL"
        app:layout_constraintStart_toStartOf="@+id/buttonFevereiro"
        app:layout_constraintTop_toTopOf="@+id/buttonMarco" />

    <Button
        android:id="@+id/buttonMaio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="MAIO"
        app:layout_constraintEnd_toStartOf="@+id/buttonJunho"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonMarco" />

    <Button
        android:id="@+id/buttonJunho"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JUNHO"
        app:layout_constraintStart_toStartOf="@+id/buttonFevereiro"
        app:layout_constraintTop_toTopOf="@+id/buttonMaio" />

    <Button
        android:id="@+id/buttonJulho"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="JULHO"
        app:layout_constraintStart_toStartOf="@+id/buttonJaneiro"
        app:layout_constraintTop_toBottomOf="@+id/buttonMaio" />

    <Button
        android:id="@+id/buttonAgosto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AGOSTO"
        app:layout_constraintStart_toStartOf="@+id/buttonFevereiro"
        app:layout_constraintTop_toTopOf="@+id/buttonJulho" />

    <Button
        android:id="@+id/buttonSetembro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="SETEMBRO"
        app:layout_constraintStart_toStartOf="@+id/buttonJaneiro"
        app:layout_constraintTop_toBottomOf="@+id/buttonJulho" />

    <Button
        android:id="@+id/buttonOutubro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OUTUBRO"
        app:layout_constraintStart_toStartOf="@+id/buttonFevereiro"
        app:layout_constraintTop_toTopOf="@+id/buttonSetembro" />

    <Button
        android:id="@+id/buttonNovembro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="NOVEMBRO"
        app:layout_constraintStart_toStartOf="@+id/buttonJaneiro"
        app:layout_constraintTop_toBottomOf="@+id/buttonSetembro" />

    <Button
        android:id="@+id/buttonDezembro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DEZEMBRO"
        app:layout_constraintStart_toStartOf="@+id/buttonFevereiro"
        app:layout_constraintTop_toTopOf="@+id/buttonNovembro" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="450dp"
        android:text="2025"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CNPJ - 233 001 50"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sitio Casa Amarela - Santa Luzia"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

</androidx.constraintlayout.widget.ConstraintLayout>